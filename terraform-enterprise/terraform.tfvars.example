# Terraform Enterprise Configuration

# AWS Configuration
aws_region = "us-west-2"

# Base Infrastructure Reference
base_project_name = "base-env"  # Must match the project name from base infrastructure

# TFE Configuration
tfe_project_name   = "tfe"
tfe_instance_type  = "t3.large"
tfe_hostname       = "terraform-enterprise.local"

# SSH Key (must exist in AWS and match base infrastructure)
key_pair_name = "vault-user-key"

# Optional: Additional CIDR blocks allowed to access TFE
# allowed_cidr_blocks = ["10.0.0.0/8", "192.168.0.0/16"]
